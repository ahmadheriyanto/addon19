<odoo>
  <record id="view_location_form_inherit_fulfillment" model="ir.ui.view">
    <field name="name">view_location_form_inherit_fulfillment</field>
    <field name="model">stock.location</field>
    <!-- Make sure this ref matches the partner form view in your Odoo 19 instance -->
    <field name="inherit_id" ref="stock.view_location_form"/>
    <field name="arch" type="xml">
      <xpath expr="//field[@name='usage']" position="before">
        <field name="main_parent"/>
      </xpath>
    </field>
  </record>
</odoo>